{
  "name": "score-service",
  "version": "1.0.0",
  "type": "module",
  "description": "Score service voor Photo Hunt app",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js --legacy-watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "npx eslint . --ext .js,.mjs,.cjs"
  },
  "dependencies": {
    "amqplib": "^0.10.8",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "form-data": "^4.0.3",
    "helmet": "^8.1.0",
    "mongoose": "^8.15.0",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@shelf/jest-mongodb": "^4.0.0",
    "@types/jest": "^29.5.8",
    "eslint": "^9.29.0",
    "eslint-plugin-jest": "^29.0.1",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.3"
  },
  "jest": {
    "preset": "@shelf/jest-mongodb",
    "testTimeout": 30000,
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
    "collectCoverageFrom": [
      "controllers/**/*.js",
      "models/**/*.js",
      "utils/**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**"
    ],
    "coverageReporters": ["text", "json", "lcov", "html"],
    "coverageDirectory": "coverage"
  }
}
